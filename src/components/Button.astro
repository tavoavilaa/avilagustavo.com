---
interface Props {
	url: string;
	text: string;
}
const { url, text } = Astro.props;
---

<a href={url}  class="transition-all duration-75 delay-0 ease-in-out text-gray-900 bg-spring-green-500 hover:bg-spring-green-300 focus:ring-4 focus:outline-none focus:ring-green-300 dark:focus:ring-green-800 shadow-sm shadow-spring-green-500/50 dark:shadow-lg dark:shadow-green-800/80 font-medium rounded-lg text-sm px-5 py-2.5 text-center">{text}</a>

